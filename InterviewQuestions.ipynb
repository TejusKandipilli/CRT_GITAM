   "source": [
    "arr = [2,8,3,5,7,4,1,2]\n",
    "\n",
    "def calculate(arr,r,unit):\n",
    "  if not arr:\n",
    "    return -1\n",
    "  \n",
    "  cur_sum = 0\n",
    "  needed = r*unit\n",
    "\n",
    "  for i in range(len(arr)):\n",
    "    cur_sum += arr[i]\n",
    "    if cur_sum >= needed:\n",
    "      return i+1\n",
    "  \n",
    "  return 0\n",
    "\n",
    "print(calculate(arr,7,2))"
   ]
  }
